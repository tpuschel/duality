{
	"name": "duality",
	"displayName": "Duality",
	"description": "Language Support for the Duality Programming Language",
	"version": "0.0.1",
	"preview": true,
	"engines": {
		"vscode": "^1.40.0"
	},
	"publisher": "puschel",
	"homepage": "https://duality-lang.org",
	"license": "MIT",
	"repository": "github:tpuschel/duality",
	"keywords": [
		"programming",
		"language",
		"lsp"
	],
	"icon": "256px-Yin_and_yang.png",
	"bugs": {
		"url": "https://github.com/tpuschel/duality/issues",
		"email": "t.hasenpusch@icloud.com"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"activationEvents": [
		"onLanguage:duality"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"vscode-languageclient": "^5.2.1"
	},
	"contributes": {
		"languages": [
			{
				"id": "duality",
				"aliases": [
					"Duality",
					"duality"
				],
				"extensions": [
					".dy"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "duality",
				"scopeName": "source.dy",
				"path": "./syntaxes/duality.tmLanguage.json"
			}
		]
	}
}