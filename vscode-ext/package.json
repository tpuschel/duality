{
	"name": "duality",
	"displayName": "Duality",
	"description": "Language Support for the Duality Programming Language",
	"version": "0.0.8",
	"preview": true,
	"engines": {
		"vscode": "^1.40.0"
	},
	"publisher": "puschel",
	"homepage": "https://duality-lang.org",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/tpuschel/duality.git",
		"directory": "vscode-ext"
	},
	"author": {
		"name": "Thorben Hasenpusch",
		"email": "t.hasenpusch@icloud.com"
	},
	"keywords": [
		"programming",
		"language",
		"lsp"
	],
	"icon": "256px-Yin_and_yang.png",
	"bugs": {
		"url": "https://github.com/tpuschel/duality/issues",
		"email": "t.hasenpusch@icloud.com"
	},
	"categories": [
		"Programming Languages",
		"Debuggers",
		"Other"
	],
	"activationEvents": [
		"onLanguage:duality",
		"onDebug"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.9.1",
		"@types/vscode": "^1.43.0",
		"glob": "^7.1.5",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"vscode-languageclient": "^6.1.1"
	},
	"contributes": {
		"languages": [
			{
				"id": "duality",
				"aliases": [
					"Duality",
					"duality",
					"dy"
				],
				"extensions": [
					".dy"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "duality",
				"scopeName": "source.dy",
				"path": "./syntaxes/duality.tmLanguage.json"
			}
		],
		"configuration": {
			"title": "Duality",
			"properties": {
				"duality.path": {
					"type": "string",
					"description": "The path to the duality executable.",
					"default": "duality"
				}
			}
		},
		"debuggers": [
			{
				"type": "duality",
				"label": "Duality Debug",
				"languages": [
					"duality"
				],
				"configurationAttributes": {
					"launch": {
						"required": [
							"program"
						],
						"properties": {
							"program": {
								"type": "string",
								"description": "The program to debug."
							}
						}
					}
				},
				"initialConfigurations": [
					{
						"type": "duality",
						"request": "launch",
						"name": "Duality Launch",
						"program": "${file}"
					}
				],
				"configurationSnippets": [
					{
						"label": "Duality: Launch",
						"description": "A new configuration for launching a Duality program.",
						"body": {
							"type": "duality",
							"request": "launch",
							"name": "Duality Launch",
							"program": "${file}"
						}
					}
				]
			}
		],
		"breakpoints": [
			{
				"language": "duality"
			}
		]
	}
}
