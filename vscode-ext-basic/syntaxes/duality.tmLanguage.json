{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Duality",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.duality",
					"match": "\\b(list|try|let|choice|do|rec)\\b"
				}
			]
		},
		"comments": {
			"patterns": [
				{
					"name": "comment.block.documentation.duality",
					"begin": "/#",
					"end": "#/"
				},
				{
					"name": "comment.line.number-sign.duality",
					"match": "(#).*$\n?"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.duality",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.duality",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.dy"
}